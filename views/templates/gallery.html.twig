<div class="gallery">
	<div class="preview">
		{% for image in images %}
			<div>
				<a target="_blank" href="/portfolio/images/{{ image }}">
					<img alt="{{ config.site.title }} {{ image }}" src="/portfolio/images/{{ image }}"/>
				</a>
			</div>
		{% endfor %}
	</div>
	<div class="list">
		{% for image in images %}
			<div>
				<img alt="{{ config.site.title }} {{ image }}" src="/portfolio/preview/{{ image }}"/>
			</div>
		{% endfor %}
	</div>
</div>
